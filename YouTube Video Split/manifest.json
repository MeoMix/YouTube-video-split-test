{
	"manifest_version": 2,
	"name": "Sample Extension",
	"description": "Splitting YouTubes video",
	"version": "1.0",
	"background": {
		"page": "background.html"
	},
	"browser_action": {
		"default_popup": "foreground.html"
	},
	"content_scripts": [{
		"matches": ["*://*.youtube.com/embed/*?enablejsapi=1&origin=chrome-extension:*"],
		"all_frames": true,
		"js": ["js/thirdParty/lodash.js", "js/thirdParty/jquery.js", "js/inject/youTubeIFrameInject.js"]
	}],
	"web_accessible_resources": [
		"js/inject/interceptor.js"
	],
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"*://*.youtube.com/*"
	],
	"content_security_policy": "script-src 'self' 'unsafe-eval' https://www.google-analytics.com/;  object-src 'self'"
}